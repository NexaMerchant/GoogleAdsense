{
    "name": "nexa-merchant/googleadsense",
    "license": "MIT",
    "type": "library",
    "description" : "GoogleAdsense Apps for NexaMerchant, Use this package to integrate Google Adsense into your application, Use Api to get data from Google Adsense",
    "keywords": ["googleadsense", "google", "adsense", "nexamerchant"],
    "suggest": {
        "laravel/framework": "Laravel Framework",
        "google/apiclient": "Google API Client",
        "phpunit/phpcov": "PHPUnit Code Coverage",
        "phpunit/phpunit": "PHPUnit"
    },
    "authors": [
        {
            "name": "Steve",
            "email": "nice.lizhi@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "~6.0||~7.0||~8.0||~9.0||~10.0||~11.0",
        "google/apiclient": "^2.7"
    },
    "require-dev": {
        "phpunit/phpcov": ">=6.0",
        "phpunit/phpunit": ">=8.0",
        "google/apiclient": "^2.7",
        "laravel/framework": "~6.0||~7.0||~8.0||~9.0||~10.0||~11.0"
    },
    "autoload": {
        "psr-4": {
            "NexaMerchant\\GoogleAdsense\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NexaMerchant\\GoogleAdsense\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NexaMerchant\\GoogleAdsense\\Providers\\GoogleAdsenseServiceProvider"
            ],
            "aliases": {}
        }
    },
    "homepage": "https://nexamerchant.github.io/docs/",
    "discussions": "https://github.com/orgs/NexaMerchant/discussions",
    "minimum-stability": "dev"
}
